      <b-form-group label=\"Email address:\">\n            <b-form-input v-model=\"email\" type=\"email\" required />\n          </b-form-group>\n\n          <b-form-group label=\"Password:\">\n            <b-form-input type=\"password\" v-model=\"password\" required/>\n          </b-form-group>\n\n          <b-form-group class=\"text-center\">\n            <b-button variant=\"success\" @click=\"signUp\">SignUp</b-button>\n            <span> or go back to <router-link to=\"/login\">login</router-link>.</span>\n          </b-form-group>\n        </b-form>\n\n      </b-col>\n    </b-row>\n  </b-container>\n\n</template>\n\n<script>\n    import firebase from 'firebase';\n    export default {\n        name: 'signUp',\n        data() {\n            return {\n                email: '',\n                password: ''\n            }\n        },\n        methods: {\n            signUp: function() {\n                firebase.auth().createUserWithEmailAndPassword(this.email, this.password).then(\n                    (user) => {\n                        console.log(\"user\", user);\n                        this.username = user.user.email;\n                        localStorage.setItem(\"username\", this.username);\n                        this.$router.replace('/');\n                    },\n                    (err) => {\n                        alert('Oops. ' + err.message)\n                    }\n                );\n            }\n        }\n    }\n</script>\n\n<style scoped>\n</style>\n"],"sourceRoot":"src/views"}]}